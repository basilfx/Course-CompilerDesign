(void)main:(String[])args 
{
    int a = 10;
    int b = 0;
    String c = @"test";

    /* Puts int a and String c on the stack */
    a;
    b;

    /* Puts nothing on the stack */
    a = b = 200;

    /* Returns an integer */
    a == a ? 10 : 2;

    /* Returns an string */
    [builtin print:@"Stack clean"];

    /* Does nothing */
    [System.out print:@" :)"];
}